[user]
	email = 
	name = 

[core]
  pager = delta

[interactive]
  diffFilter = delta --color-only

[delta]
  navigate = true       # use n and N to move between diff sections
  light = false         # set to true if you're in a terminal w/ a light background color (e.g. the default macOS terminal)
  true-color = always   # enable 24-bit color
  #syntax-theme = Solarized (dark)
  syntax-theme = Visual Studio Dark+
  # For removed lines, set foreground color to syntax colors, background color to hex
  minus-style = syntax "#300e2f"
  # For added lines, set foreground color to syntax colors, background color to hex
  plus-style = syntax "#0e1e30"

[merge]
  conflictstyle = diff3

[diff]
  colorMoved = default
